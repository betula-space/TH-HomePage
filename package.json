{
  "name": "th-homepage",
  "version": "0.0.0",
  "description": "TrialHammer HomePage Project",
  "scripts": {
    "serve": "tsc --watch & live-server --quiet --port=3000 ./src",
    "build": "tsc",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix"
  },
  "devDependencies": {
    "f2elint": "^2.2.1",
    "live-server": "^1.2.2",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  }
}
